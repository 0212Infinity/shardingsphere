<?xml version="1.0" encoding="UTF-8"?>
<asserts>
    <assert id="assertDeleteWithAlias" parameters="1000,1100">
        <tables>
            <table name="t_order" alias="o" />
        </tables>
        <conditions>
            <condition column-name="order_id" table-name="t_order" operator="BETWEEN">
                <value index="0" literal="1000" type="int" />
                <value index="1" literal="1100" type="int" />
            </condition>
        </conditions>
    </assert>
    <assert id="assertDeleteWithoutAlias" parameters="1000,1001,'init'">
        <tables>
            <table name="t_order"/>
        </tables>
        <conditions>
            <condition column-name="order_id" table-name="t_order" operator="EQUAL">
                <value index="0" literal="1000" type="int" />
            </condition>
            <condition column-name="user_id" table-name="t_order" operator="EQUAL">
                <value index="1" literal="1001" type="int" />
            </condition>
        </conditions>
    </assert>
    <assert id="assertDeleteWithoutShardingValue" parameters="'init'">
        <tables>
            <table name="t_order" />
        </tables>
    </assert>
</asserts>
